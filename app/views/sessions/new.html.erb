<h1> New Sessions </h1>

<h1>Sign In</h2>
  <%= form_tag sessions_path do %>

  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>

  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password%>
  </p>

  <p>
    <%= submit_tag "Sign In", class: "btn btn-primary" %>
    or
    <%= link_to "Sign Up", signup_path, class: "btn btn-success" %>
  </p>

  <%end%>
